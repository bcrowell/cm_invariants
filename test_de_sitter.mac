/* vacuum-dominated FLRW metric */

load(ctensor);

ct_coords:[t,r,theta,phi];
a : sinh(t); /* t=c*T, where c=sqrt(Lambda/3) and T is the usual FLRW time coordinate*/
k : -1;
lg:matrix([1,0,0,0],
          [0,-a^2*1/(1-k*r^2),0,0],
          [0,0,-a^2*r^2,0],
          [0,0,0,-a^2*r^2*sin(theta)^2]);
load("cm.mac");

cm_print_invariants();
